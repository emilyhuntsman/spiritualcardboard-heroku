    <div class="container-fluid show-container">
        <div class="padding"></div>
        <div class="col text-center">
            <img class="show-img" src="<%=print.imgSrc%>"/>
            <h3 class="normal-font"><%=print.name%></h3>
            <h4 class="noraml-font">$<%=print.price%></h4>
            <button type="button" class="btn btn-outline-secondary" onclick="location.href = '/prints/'">back</button>
